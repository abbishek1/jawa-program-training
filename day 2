//program for positive or negative
public class Exercise{
  public static void main(String[]args){
    int num=89;
if(num>0){
  System.out.println("positive");}
else if(num<0){
  System.out.println("negative");}
else
{
  System.out.prinln("zero")
    }
  }
}
